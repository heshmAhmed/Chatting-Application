<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_RIGHT" style="-fx-background-color: #1A1A40;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="contactsList" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: transparent;" />
      <Button fx:id="addButton" mnemonicParsing="false" onAction="#handleAddButton" stylesheets="@../../../styles/main-style.css" text="add">
         <VBox.margin>
            <Insets bottom="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
