<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<fx:root fx:id="groupHBox" onMouseClicked="#handleGroupBox" styleClass="body" stylesheets="@../../../styles/contact-box-style.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="46.0" prefWidth="39.0">
         <children>
            <Circle fx:id="groupPhotoCircle" fill="DODGERBLUE" layoutX="27.0" layoutY="23.0" nodeOrientation="LEFT_TO_RIGHT" radius="22.0" stroke="DODGERBLUE" strokeType="INSIDE" strokeWidth="0.0" />
         </children>
      </AnchorPane>
      <VBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="138.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="groupNameLabel" text="contactName" textFill="WHITE">
               <VBox.margin>
                  <Insets top="3.0" />
               </VBox.margin>
            </Label>
         </children>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </VBox>
   </children>
   <padding>
      <Insets bottom="3.0" top="3.0" />
   </padding>
</fx:root>
